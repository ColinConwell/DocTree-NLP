    {
      "id": "199d606e-3bdf-80ca-9069-cd6e36f6f2e9",
      "type": "code",
      "content": "javascript\nfunction notionToMarkdown(block) {\n    switch(block.type) {\n        case 'heading_1':\n            return `# ${block.heading_1.text[0].plain_text}\\n`;\n        case 'paragraph':\n            return `${block.paragraph.text[0].plain_text}\\n`;\n        case 'bulleted_list_item':\n            return `- ${block.bulleted_list_item.text[0].plain_text}\\n`;\n        // Add more conversions as needed\n    }\n}\n",
      "has_children": false,
      "indent_level": 0
    },
